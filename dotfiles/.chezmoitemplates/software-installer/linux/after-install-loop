chezmoi:template:missing-key=zero
{{- $versions := (includeTemplate "software" . | fromYaml).versions }}
{{- includeTemplate "software-installer/linux/install-package" (dict
      "name" "lunarvim"
      "spec" (dict)
      "install" true
      "install_command" "install-lunarvim"
    )
}}

{{- includeTemplate "software-installer/linux/install-package" (dict
      "name" "dotnet"
      "spec" (dict)
      "install" .devtools
      "install_command" (cat "install-dotnet" (kindIs "slice" $versions.dotnet | ternary ($versions.dotnet | join " ") $versions.dotnet))
    )
}}

if [ -n "$FAILED_TOOLS" ]
then
  echo $'\n'"Some tools were not installed due to missing dependencies or errors:"$'\n'"$FAILED_TOOLS" >&2
fi

